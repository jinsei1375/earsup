{
  "name": "earsup",
  "main": "expo-router/entry",
  "version": "1.0.15",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "prebuild:clean": "rm -rf ios/ android/ .expo/",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "pods:install": "cd ios && pod install && cd ..",
    "ios:setup": "npm run prebuild:clean && npm run prebuild:ios && npm run pods:install",
    "deploy:testflight": "npm run version:patch && npm run ios:setup && npm run ios:archive",
    "ios:archive": "echo '📱 app.jsonのversionを合わせてXcodeで Product → Archive → TestFlight アップロードを実行してください' && open ios/earsup.xcworkspace"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "^54.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.6",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-tracking-transparency": "~6.0.7",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-google-mobile-ads": "^15.7.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.11",
    "react-native-worklets": "^0.5.1",
    "react-test-renderer": "19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}